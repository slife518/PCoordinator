<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/ScrollView1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.company.jk.pcoordinator.mypage.MypageFragment">

        <!-- 툴바, 추가된 소스 -->
        <android.support.v7.widget.Toolbar
            android:id="@+id/my_toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="?attr/colorPrimary"
            android:elevation="4dp"
            android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
            app:popupTheme="@style/ThemeOverlay.AppCompat.Light"/>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@color/searchBackground"
                    android:orientation="vertical">
                        <TextView
                            android:id="@+id/tv_myinfo"
                            android:text="@string/txCustomerinfo"
                            android:layout_height="70dp"
                            android:drawableStart="@drawable/ic_baseline_insert_drive_file_24px"
                            android:layout_marginTop="@dimen/app_margin_top"
                            style="@style/Mypagebutton"/>

                        <TextView
                            android:id="@+id/tv_mybabyinfo"
                            android:text="@string/mybabyinfo"
                            android:drawableStart="@drawable/ic_baseline_accessibility_24px"
                            android:layout_height="70dp"
                            android:layout_marginTop="1dp"
                            style="@style/Mypagebutton"/>

                        <TextView
                            android:id="@+id/tv_parentslist"
                            android:text="@string/parentlist"
                            android:drawableStart="@drawable/ic_baseline_group_24px"
                            android:layout_height="70dp"
                            android:layout_marginTop="@dimen/app_margin_top"
                            style="@style/Mypagebutton"/>

                        <TextView
                            android:id="@+id/tv_password"
                            android:text="@string/txPassword"
                            android:drawableStart="@drawable/ic_baseline_phonelink_lock_24px"
                            android:layout_height="70dp"
                            android:layout_marginTop="@dimen/app_margin_top"
                            style="@style/Mypagebutton"/>

                        <TextView
                            android:id="@+id/tv_Logout"
                            android:text="@string/logout"
                            android:drawableStart="@drawable/ic_baseline_clear_24px"
                            android:layout_height="70dp"
                            android:layout_marginTop="1dp"
                            style="@style/Mypagebutton"/>

                        <TextView
                            android:id="@+id/tv_QnA"
                            android:text="@string/QnA"
                            android:drawableStart="@drawable/ic_outline_question_answer_24px"
                            android:layout_height="70dp"
                            android:layout_marginTop="1dp"
                            style="@style/Mypagebutton"/>
                        <TextView
                            android:id="@+id/tv_review"
                            android:text="@string/review"
                            android:drawableStart="@drawable/ic_baseline_question_answer_24px"
                            android:layout_height="70dp"
                            android:layout_marginTop="1dp"
                            style="@style/Mypagebutton"/>

                        <!--<Button-->
