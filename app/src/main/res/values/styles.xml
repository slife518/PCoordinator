    <style name="CustomTitleTextAppearance">
        <item name="android:textSize">12dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="EditIcon">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">8dp</item>
        <item name="android:layout_marginRight">8dp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="MyWideButton">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@color/buttonColorAccent</item>
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/btn_middlesize</item>
    </style>

    <style name="btn_style">
        <item name="android:background">@drawable/button_bg</item>
        <item name="android:minHeight">46dp</item>
        <item name="android:textSize">@dimen/btn_middlesize</item>
        <item name="android:paddingLeft">16dp</item>
    </style>

    <style name="Mypagebutton">
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:drawableEnd">@drawable/ic_baseline_keyboard_arrow_right_24px</item>
        <item name="android:textSize">@dimen/btn_bigsize</item>
        <item name="android:paddingLeft">16dp</item>
        <item name="android:paddingRight">8dp</item>
        <item name="android:background">@android:color/white</item>
        <item name="android:fontFamily">@font/nanumsquarer</item>
        <item name="android:drawablePadding">@dimen/appbar_padding_left</item>
    </style>

    <style name="toolbarTitle">
        <item name="android:fontFamily">@font/nanumsquareeb</item>
    </style>

