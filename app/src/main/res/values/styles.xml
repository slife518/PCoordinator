    <style name="IconBack40x40">
        <item name="android:src">@drawable/ic_baseline_keyboard_arrow_left_24px</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">40dp</item>sss
        <item name="android:layout_height">40dp</item>
    </style>
    <style name="IconExit40x40">
        <item name="android:src">@drawable/ic_baseline_clear_24px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@color/transparent</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginLeft">16dp</item>
    </style>
    <style name="IconForward40x40">
        <item name="android:src">@drawable/ic_baseline_keyboard_arrow_right_24px</item>
        <!--<item name="app:layout_constraintRight_toRightOf">parent</item>-->
        <!--<item name="android:gravity">center</item>-->
        <item name="android:layout_width">40dp</item>
        <item name="android:layout_height">40dp</item>
    </style>
    <style name="Mypagebutton">
        <item name="android:background">@drawable/button_border</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:fontFamily">fonts/nanumsquarel</item>
        <item name="android:textSize">16dp</item>
        <item name="android:paddingLeft">20dp</item>

    </style>
    <style name="ImageButtonForward">
        <item name="android:src">@drawable/ic_baseline_keyboard_arrow_right_24px</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">2dip</item>
        <item name="android:layout_marginBottom">2dip</item>
        <!--<item name="android:background">null</item>-->
        <item name="android:padding">10dip</item>
    </style>

    <style name="CustomTitleTextAppearance">
        <item name="android:textSize">23sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ToolbarTheme">
        <item name="android:fontFamily">fonts/nanumsquarel</item>
        <!--<item name="android:fontFamily">fonts/greatvibesregular</item>-->
    </style>

    <style name="EditIcon">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_marginLeft">16dp</item>
        <item name="android:layout_marginRight">16dp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>

    <style name="MyRadioButtonStyle" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/custom_btn_radio</item>
    </style>

