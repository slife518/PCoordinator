<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.Myapptheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="actionBarItemBackground">@drawable/selectable_background_myapptheme</item>
        <item name="popupMenuStyle">@style/PopupMenu.Myapptheme</item>
        <item name="dropDownListViewStyle">@style/DropDownListView.Myapptheme</item>
        <item name="actionBarTabStyle">@style/ActionBarTabStyle.Myapptheme</item>
        <item name="actionDropDownStyle">@style/DropDownNav.Myapptheme</item>
        <item name="actionBarStyle">@style/ActionBar.Solid.Myapptheme</item>
        <item name="actionModeBackground">@drawable/cab_background_top_myapptheme</item>
        <item name="actionModeSplitBackground">@drawable/cab_background_bottom_myapptheme</item>
        <item name="actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Myapptheme</item>


    </style>

    <style name="ActionBar.Solid.Myapptheme" parent="@style/Widget.AppCompat.Light.ActionBar.Solid">
        <item name="background">@drawable/ab_solid_myapptheme</item>
        <item name="backgroundStacked">@drawable/ab_stacked_solid_myapptheme</item>
        <item name="backgroundSplit">@drawable/ab_bottom_solid_myapptheme</item>
        <item name="progressBarStyle">@style/ProgressBar.Myapptheme</item>
    </style>

    <style name="PopupMenu.Myapptheme" parent="@style/Widget.AppCompat.Light.PopupMenu">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_myapptheme</item>
    </style>

    <style name="DropDownListView.Myapptheme" parent="@style/Widget.AppCompat.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_myapptheme</item>
    </style>

    <style name="ActionBarTabStyle.Myapptheme" parent="@style/Widget.AppCompat.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_myapptheme</item>
    </style>

    <style name="DropDownNav.Myapptheme" parent="@style/Widget.AppCompat.Light.Spinner.DropDown.ActionBar">
        <item name="android:background">@drawable/spinner_background_ab_myapptheme</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_myapptheme</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_myapptheme</item>
    </style>

    <style name="ProgressBar.Myapptheme" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_myapptheme</item>
    </style>

    <style name="ActionButton.CloseMode.Myapptheme" parent="@style/Widget.AppCompat.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_myapptheme</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->


